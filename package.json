{
  "name": "react-jest-snapshot-helper",
  "version": "1.0.1",
  "description":
    "Helper class to simplify Jest snapshot testing of React components with a variety of props combinations.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore components,test.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "lint": "esw src",
    "prebuild": "npm-run-all clean-dist test lint",
    "remove-dist": "rimraf ./dist",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianSchroer/react-jest-snapshot-helper.git"
  },
  "keywords": ["react", "jest", "test", "testing", "snapshot", "snapshots"],
  "author": "Brian Schroer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BrianSchroer/react-jest-snapshot-helper/issues"
  },
  "homepage":
    "https://github.com/BrianSchroer/react-jest-snapshot-helper#readme",
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "jest": "^22.1.4",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "jest": ">=15 <23",
    "react": ">=15 <17"
  }
}
